/* custom-tab-bar/index.wxss */
.tab-bar {
  position: fixed;
  border: 1px solid #E15B99;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 50rpx;
  border-radius: 50%;
  height: 48px;
  background-color: white;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  /* font-family: Fira Sans; */
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-top: 20rpx;
}

.tab-bar-item-cover-image {
  width: 27px;
  height: 27px;
}

.tab-bar-item-cover-view {
  font-size: 12px;
}